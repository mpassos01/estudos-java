package entities;

public class Employee {
	public String name;
	public double grossSalary;
	public double tax;
	public double porcentagem;
	
	public double salary() {
		return grossSalary - tax;
	}

	public String toString() {
		return name + ", $ " + salary();		
	}
	
	public void increaseSalary() {
		return (salary() * porcentagem):100;
	}
	
	
}
