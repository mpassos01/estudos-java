import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int N, inicio, quadrado, cubo;
		N = sc.nextInt();
		
		for (int i = 1; i <= N; i++) {
			
			inicio = i;
			quadrado = i * i;
			cubo = i * i * i;
			
			System.out.printf("%d %d %d", inicio, quadrado, cubo);
			
			
		}
		
		sc.close();
	}
}